        "77056"
      ]
    },
    {
      "city": "Dallas",
      "state": "TX",
      "zips": [
        "75201",
        "75204",
        "75205",
        "75214",
        "75230"
      ]
    },
    {
      "city": "Phoenix",
      "state": "AZ",
      "zips": [
        "85004",
